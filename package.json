{
  "name": "crossecore",
  "version": "0.1.2",
  "description": "Ecore and OCL runtime API for TypeScript",
  "license": "EPL-2.0",
  "directoriess": {
    "src": "src",
    "test": "test"
  },
  "files": ["dist", "LICENSE", "README.md"],
  "jest": {
    "testEnvironment": "node"
  },
  "main": "dist/crossecore.cjs.min.js",
  "scripts": {
    "test": "jest --coverage",
    "madge": "madge --circular src/index.ts",
